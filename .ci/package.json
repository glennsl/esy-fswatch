{
  "name": "esy-fswatch-prebuilt",
  "version": "1.13.0004",
  "esy": {
    "build": [
      [
        "cp",
        "-R",
        "#{ '.' / 'install-'os / 'lib' }",
        "#{ self.install }"
      ],
      [
        "cp",
        "-R",
        "#{ '.' / 'install-'os / 'bin' }",
        "#{ self.install }"
      ],
      [
        "cp",
        "-R",
        "#{ '.' / 'install-'os / 'include' }",
        "#{ self.install / 'include' }"
      ]
    ],
    "exportedEnv": {
      "FSWATCH_INC": {
        "val": "-I#{self.install / 'include' / 'libfswatch' / 'c'}",
        "scope": "global"
      },
      "LIBRARY_PATH": {
        "val": "#{self.install / 'lib'}",
        "scope": "global"
      },
      "PATH": {
        "val": "#{self.install / 'bin'}#{os == 'windows' ? ';' : ':'}$PATH",
        "scope": "global"
      }
    }
  }
}
